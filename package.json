{
  "name": "my-package-name",
  "version": "0.0.1",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "node --watch --inspect --loader ts-node/esm -- src/main.ts | pino-pretty",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "git": "husky",
    "prepare": "run-s build git",
    "format": "prettier --check '{src,test}/**/*.ts'",
    "format:fix": "prettier --write '{src,test}/**/*.ts'",
    "lint": "eslint --ext ts '{src,test}/**'",
    "lint:fix": "eslint --fix --ext ts '{src,test}/**'",
    "test": "node --test --loader ts-node/esm -- ./test/*.test.ts"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "pino": "^9.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": "^20",
    "npm": "^10"
  },
  "type": "module",
  "description": "",
  "author": "Roberto Sero",
  "exports": {
    "default": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "bin": "./bin",
  "files": [
    "dist/**"
  ],
  "license": "MIT",
  "volta": {
    "node": "20.13.1"
  }
}
